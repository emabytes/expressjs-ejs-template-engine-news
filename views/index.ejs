<%-include('./partials/head')%>

<body>
    <h1>NEWS</h1>
    <section>
        <%data.forEach(elt => {%>
        <article>
            <h3>
                <%= elt.title %>
            </h3>
            <img src="<%= elt.urlToImage %>" alt=""></img>
            <p><%= elt.description %></p>
            <div class="flex">
                <h4><%= elt.author %></h4>
                <a href="<%= elt.url %>">READ MORE</a>
            </div>

        </article>
        <%})%>
    </section>
</body>

</html>